{
  "rewrites": [
    {
      "source": "/",
      "destination": "https://github.com/GLASS20"
    },
    {
      "source": "/:match",
      "destination": "https://github.com/:match"
    }
  ],
  "redirects": [
    { 
      "source": "/:match/:url", 
      "destination": "https://github.com/:match/:url", 
      "permanent": true 
    }
  ]
}
